{
  "apiVersion": "apps/v1",
  "kind": "Deployment",
  "metadata": {
    "name": "SprintBootApp",
    "labels": {
      "language": "Java",
      "app": "spring-boot-app"
    }
  },
  "spec": {
    "replicas": 1,
    "selector": {
      "matchLabels": {
        "language": "Java",
        "app": "spring-boot-app"
      }
    },
    "template": {
      "metadata": {
        "labels": {
          "language": "Java",
          "app": "spring-boot-app"
        }
      },
      "spec": {
        "containers": [
          {
            "name": "SprintBootApp",
            "image": "my-springboot-app:v1.0",
            "ports": [
              {
                "containerPort": 8080,
                "protocol": "TCP"
              }
            ]
          }
        ]
      }
    }
  }
}
